{
  "name": "backend",
  "version": "0.1.0",
  "description": "Express Backend for Fullstack NextJS App",
  "main": "server.ts",
  "type": "commonjs",
  "engines": {
    "node": ">= 16.15.0",
    "npm": ">=8.5.5"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "concurrently \"tsc --watch\" \"npm run start\"",
    "start": "nodemon -q dist/server.js",
    "client": "cd ../ && npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "",
  "license": "MPL2",
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.5.2",
    "prisma": "^4.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.2",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
